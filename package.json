{
	"name": "@toreda/types",
	"version": "2.0.0",
	"public": true,
	"description": "Common & convenient mappings, aliases, and types used in Toreda TypeScript packages.",
	"main": "./dist/index.js",
	"typings": "./dist/index.d.ts",
	"scripts": {
		"lint": "yarn lint:nofix",
		"lint:nofix": "yarn lint",
		"lint:fix": "yarn eslint src/**/*.{js,ts,tsx} --quiet --fix",
		"build": "yarn gulp",
		"make:docs": "yarn typedoc docs --categorizeByGroup false --entryPoints ./src/index.ts",
		"make:depgraph": "yarn madge src/index.ts --image dependency-graph.png"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/toreda/types.git"
	},
	"keywords": [
		"JavaScript",
		"lib",
		"node",
		"Toreda",
		"types",
		"TypeScript"
	],
	"author": "Toreda, Inc.",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/toreda/types/issues"
	},
	"homepage": "https://github.com/toreda/types#readme",
	"sideEffects": false,
	"dependencies": {},
	"devDependencies": {
		"@swc/core": "^1.2.84",
		"@swc/wasm": "^1.2.84",
		"@toreda/prettier-config": "^1.0.1",
		"@types/gulp": "^4.0.9",
		"@types/node": "^16.7.10",
		"del": "^6.0.0",
		"gulp": "^4.0.2",
		"gulp-typescript": "^6.0.0-alpha.1",
		"prettier": "^2.4.1",
		"ts-node": "^10.2.1",
		"ts-register": "^0.1.0",
		"typedoc": "^0.22.4",
		"typescript": "^4.4.2"
	},
	"prettier": "@toreda/prettier-config"
}
